<?xml version="1.0"?>

<settings>
	<project_title value = "The Core Visualizer"/>

	<data_recorder_out_file_loc 
		value = "./sample_data/data_recorder_out.txt"/>

	<processor_coordinate_file_loc 
		value = "./settings/processor_coordinate.csv"/>

	<graph_settings>
		<num_of_graphs value="4"/>
		<graph
			id = "2"
			y_label = "Activity"
			x_label = "Time(ps)"
			scale_factor = "2.5"
			data_field = "activity"
		/>
		<graph
			id = "1"
			y_label = "Stalled State"
			x_label = "Time(ps)"
			scale_factor = "2.5"
			data_field = "stalled"
		/>
		<graph
			id = "3"
			y_label = "Utilization"
			x_label = "Time(ps)"
			scale_factor = "2.5"
			data_field = "utilization"
		/>
		<graph
			id = "4"
			y_label = "Power (mW)"
			x_label = "Time(ps)"
			scale_factor = "0.1"
			data_field = "power_mW"
		/>
	</graph_settings>
</settings>